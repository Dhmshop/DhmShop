<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/white"
    tools:context=".view.mine.myshop.MyshopActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="我的店铺"
        android:textSize="@dimen/sp_21"
        android:gravity="center"
        android:textColor="@color/mainblack"
        android:layout_marginTop="@dimen/dp_9"
        />
    <ImageView
        android:id="@+id/back"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_31"
        android:src="@mipmap/common_back"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_7"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#E5E5E5"
        android:layout_below="@id/back"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/commandes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="小店订单：0"
        android:textSize="@dimen/sp_16"
        android:layout_below="@id/back"
        android:layout_marginTop="@dimen/dp_25"
        android:layout_marginLeft="@dimen/dp_40"
        android:textColor="@color/mainblack"
        />
    <TextView
        android:id="@+id/gain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="小店收益：0￥"
        android:textColor="@color/mainblack"
        android:textSize="@dimen/sp_16"
        android:layout_alignParentRight="true"
        android:layout_below="@id/back"
        android:layout_marginTop="@dimen/dp_25"
        android:layout_marginRight="@dimen/dp_40"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#E5E5E5"
        android:layout_below="@id/gain"
        android:layout_marginTop="@dimen/dp_12"
        />

    <LinearLayout
        android:id="@+id/q"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/commandes"
        android:layout_marginTop="@dimen/dp_35"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginBottom="@dimen/dp_16"
        >
        <LinearLayout
            android:id="@+id/topay"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_22"
                android:layout_height="@dimen/dp_26"
                android:background="@mipmap/dingdan"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="小店订单"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/togo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_25"
                android:layout_height="@dimen/dp_25"
                android:background="@mipmap/zhangdan"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="店铺账单"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/smail_shop"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_25"
                android:layout_height="@dimen/dp_23"
                android:background="@mipmap/seeshop"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="查看微店"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/toping"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_22"
                android:background="@mipmap/showshop"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="小店上货"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/setting"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_25"
                android:layout_height="@dimen/dp_25"
                android:background="@mipmap/guanli"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="店铺设置"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/q"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginBottom="@dimen/dp_16"
        >
        <LinearLayout
            android:id="@+id/cui"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_29"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/laba"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="查看催发货"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ad"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_25"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/ad"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="广告位"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/getmoney"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/money"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="提现"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/newWork"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/newer"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="新手必看"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:textColor="@color/mainblack"
                />
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>